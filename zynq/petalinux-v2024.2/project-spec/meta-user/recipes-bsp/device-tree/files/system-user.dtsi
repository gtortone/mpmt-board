/include/ "system-conf.dtsi"

&amba {
  ptm@f889d000 {
    cpu = <&cpu0>;
  };
};

&qspi {
 #address-cells = <1>;
 #size-cells = <0>;

 flash0: flash@0 {
  compatible = "spansion,s25fl256s1";
  spi-tx-bus-width = <1>;
  spi-rx-bus-width = <4>;
  spi-max-frequency = <10000000>;
  reg = <0x0>;
  #address-cells = <1>;
  #size-cells = <1>;

  partition@0x0{
   label = "bootbin";
   // start: 0          end: 0x6B0000     size: 0x6B0000    (7012352 bytes ~7MB)
   reg = <0x0 0x6B0000>;
  };

  partition@0x6C0000{
   label = "fitimage";
   // start: 0x6C0000   end: 0x1FE0000    size: 0x1920000   (26345472 bytes ~26MB)
   reg = <0x6C0000 0x1920000>;
  };

  // start: 0x1FF0000   end: 0x2000000    size: 0x10000     (65536 bytes 64kB)
  partition@0x1FF0000{
   label = "user";
   reg = <0x1FF0000 0x10000>;
  };
 };
};

&gem0 {
   status = "okay";
   phy-handle = <&phy0>;
   phy-mode = "rgmii-id";
   phy0: phy@0 {
      /*compatible = "marvell,88e1510"; */
      device_type = "ethernet-phy";
      reg = <0x0>;
      /*
       * modify PHY registers to:
       * Reg 3_16
       * LED[1] bits [7:4] = ON=link, BLINK=activity, OFF=no activity
       * LED[0] bits [3:0] = link speed detect (3 blinks= 1Gbps, 2 blinks = 100Mbps, 1 blink = 10Mbps)
       * Reg 3_17
       * LED[1:0] = ON-drive low, OFF-high
       * Reg 3_18
       * LED[0] bits [3:2] = link speed LED blink off period = 170ms
       * LED[0] bits [1:0] = link speed LED blink on period = 340ms
       */
      marvell,reg-init = <3 16 0xff00 0x42>,
               <3 17 0xfff0 0x0>,
               <3 18 0xfff0 0x6>;
   };
};

&RunControl_0 {
   compatible = "generic-uio";
};

/ {
   dma_proxy {
      compatible ="xlnx,dma_proxy";
      dmas = <&axi_dma_0 1>;
      dma-names = "dma_proxy_rx";
   };
};
